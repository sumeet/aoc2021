inp y
add y 10
-- at this point y = inp[0]+10
add z y
-- at this point, z = inp[0]+10


inp y
mul z 26 -- at this point, z = (inp[0]+10)*26
add y 5 -- (input[1]+5)
add z y

-- at this point, z = ((inp[0]+10)*26) + (input[1]+5)


inp y
mul z 26
add y 12
add z y

-- at this point,
-- z = ((((inp[0]+10)*26) + (input[1]+5))*26) + (input[2]+12)



